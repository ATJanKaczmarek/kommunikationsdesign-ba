@book{hodgeKurzeGeschichteArchitektur2022,
  title      = {Eine kurze Geschichte der Architektur: ein Überblick über die wichtigsten Stile, Bauwerke, Elemente und Materialien},
  shorttitle = {Eine kurze Geschichte der Architektur},
  author     = {Hodge, Susie},
  translator = {Pasquay, Sarah},
  date       = {2022},
  edition    = {3. Auflage},
  publisher  = {Laurence King Verlag},
  location   = {Berlin},
  abstract   = {In diesem Band der eingeführten Reihe (zuletzt "Eine kurze Geschichte der modernen Kunst", ID-A 37/19) stellt die Kunsthistorikerin Susie Hodge kompakt und gut verständlich die Architekturgeschichte der vergangenen 4.000 Jahre dar. Eine ebenso simple wie innovative Gliederung in vier Teile vereinfacht die Orientierung: Stile, Bauwerke, Elemente und Materialien. Jeder Teil ist in sich chronologisch sortiert. Von den Pyramiden, Kathedralen bis zu Wolkenkratzern und Biosphären untersucht das Buch 50 Bauwerke der Weltarchitektur auf je einer Doppelseite. Zahlreiche Querverweise zwischen den Teilen leiten den Leser zu ergänzenden Einträgen und steigern das Lesevergnügen. Infokästen geben knappe Hinweise zu biografischen Details der Architekten und zu bedeutenden Entwicklungen. Sowohl das Titelbild als auch die Fotos der Bauwerke sind ansprechend gewählt. Ein kleines Glossar sowie ein Register helfen, sich rasch zurechtzufinden. Vor "Meisterwerke der Architektur" (ID-A 20/17) und neben "Architektur für Einsteiger" (ID-A 12/11) vielen Büchereien empfohlen. (1 S) StO Kunst klein, StO Sachliteratur 1000},
  isbn       = {978-3-96244-110-4},
  langid     = {german},
  pagetotal  = {224}
}

